<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/ file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <item label="Edit68K">
            <action name="Execute">
                <execute>/usr/bin/wine /opt/EASy68K/EDIT68K.exe</execute>
            </action>
        </item>
        <item label="EASyBIN">
            <action name="Execute">
                <execute>/usr/bin/wine /opt/EASy68K/EASyBIN.exe</execute>
            </action>
        </item>
        <item label="SIM68K">
            <action name="Execute">
                <execute>/usr/bin/wine /opt/EASy68K/SIM68K.exe</execute>
            </action>
        </item>
        <item label="Terminal">
            <action name="Execute">
                <execute>/usr/bin/x-terminal-emulator</execute>
            </action>
        </item>
        <item label="Htop">
            <action name="Execute">
                <execute>/usr/bin/x-terminal-emulator -e htop</execute>
            </action>
        </item>
    </menu>
</openbox_menu>
